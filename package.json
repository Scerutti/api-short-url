  },
  "keywords": [
    "typescript",
    "nestjs"
  ],
  "scripts": {
    "build": "nest build",
    "git:push": "sh automatic-git.sh",
    "lint": "eslint --ext .ts,.tsx src/",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky",
    "rimraf": "rimraf node_modules package-lock.json && npm i",
